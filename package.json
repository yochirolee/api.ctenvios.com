{
	"name": "api-ctenvios-prisma",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"build": "tsc",
		"start": "ts-node src/server.ts",
		"postinstall": "prisma generate",
		"dev": "nodemon --exec tsx watch src/server.ts",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:stress": "jest --testPathPatterns=stress --verbose --runInBand",
		"test:stress-concurrent": "jest --testPathPatterns=stress --verbose --maxWorkers=4",
		"test:performance": "jest --testPathPatterns=performance --verbose --runInBand",
		"test:optimized": "jest src/tests/manual/testOptimizedEndpoint.test.ts --verbose",
		"test:optimized-stress": "jest src/tests/stress/optimizedStress.test.ts --verbose --runInBand",
		"test:optimized-concurrent": "jest src/tests/stress/optimizedStress.test.ts --verbose --maxWorkers=4",
		"artillery:test": "artillery run artillery-test.yml",
		"artillery:light": "artillery run artillery-light.yml",
		"artillery:optimized": "artillery run artillery-optimized.yml",
		"artillery:quick": "artillery quick --count 50 --num 5 http://localhost:3000/api/v1/invoices",
		"seed": "tsx prisma/seed.ts",
		"seed-provinces": "tsx prisma/province-cities.seed.ts",
		"seed-receipts": "tsx prisma/recepit-seed.ts",
		"seed-customers": "tsx prisma/customers-seed.ts",
		"seed-customs-rates": "tsx prisma/customsRates.seed.ts",
		"generate": "npx prisma migrate reset --preview-feature && npx prisma migrate deploy"
	},
	"keywords": [],
	"author": "Yochiro Lee Cruz",
	"license": "ISC",
	"devDependencies": {
		"@types/compression": "^1.8.1",
		"@types/cors": "^2.8.18",
		"@types/express": "^5.0.1",
		"@types/jest": "^30.0.0",
		"@types/morgan": "^1.9.9",
		"@types/node": "^22.15.24",
		"@types/supertest": "^6.0.3",
		"artillery": "^2.0.24",
		"jest": "^30.1.3",
		"nodemon": "^3.1.10",
		"prisma": "^6.14.0",
		"supertest": "^7.1.4",
		"ts-jest": "^29.4.2",
		"ts-node": "^10.9.2",
		"tsx": "^4.19.4"
	},
	"dependencies": {
		"@faker-js/faker": "^9.8.0",
		"@prisma/client": "^6.14.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/pdfkit": "^0.14.0",
		"@types/qrcode": "^1.5.5",
		"better-auth": "1.1.4",
		"bwip-js": "^4.6.0",
		"compression": "^1.8.0",
		"cors": "^2.8.5",
		"date-fns": "^4.1.0",
		"express": "^5.1.0",
		"helmet": "^8.1.0",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.0",
		"mysql2": "^3.14.2",
		"pdfkit": "^0.17.1",
		"qrcode": "^1.5.4",
		"resend": "^6.0.1",
		"twilio": "^5.8.0",
		"typescript": "^5.8.3",
		"zod": "^3.24.4"
	},
	"description": ""
}
